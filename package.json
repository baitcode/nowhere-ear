{
  "name": "nowhere-ear",
  "version": "1.0.0",
  "description": "Web interface for the Universal Ear project",
  "main": "web/server.js",
  "scripts": {
    "server": "node web/server.js",
    "start": "babel-watch --watch web web/server.js"
  },
  "watch": {
    "server": {
      "patterns": [
        "web",
        "static"
      ],
      "extensions": "js,html,css"
    }
